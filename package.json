{
  "name": "formforge",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit -p tsconfig.app.json && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{vue,ts,js,scss,css}\"",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@quasar/extras": "^1.17.0",
    "@supabase/supabase-js": "^2.89.0",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "quasar": "^2.18.6",
    "uuid": "^13.0.0",
    "vue": "^3.5.24",
    "vue-router": "^4.6.4",
    "vuedraggable": "^4.1.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@quasar/vite-plugin": "^1.10.0",
    "@types/node": "^24.10.1",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^4.0.16",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.6.2",
    "happy-dom": "^20.0.11",
    "prettier": "^3.7.4",
    "sass": "^1.97.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.51.0",
    "vite": "^7.2.4",
    "vitest": "^4.0.16",
    "vue-tsc": "^3.1.4"
  }
}
